(define (quicksort lst)
    (if (null? lst)
        '()
        (let ((pivot (car lst))
                ï¼ˆrest (cdr lst))
                )
                (append (quicksort (filter (lambda (x) (< x pivot)) rest))
                        (list pivot)
                        (quicksort (filter (lambda (x) (>= x pivot)) rest))
                        )))
(define ls '(12 71 2 15 29 82 87 8 18 66 81 25 63 97 40 3 93 58 53 31 47))
(define res (quicksort ls))
(print res)
